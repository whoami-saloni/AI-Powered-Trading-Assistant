{
  "manifest_version": 3,
  "name": "TradingView Scraper",
  "version": "1.3",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://127.0.0.1:5000/*",
    "https://*.tradingview.com/*"
  ],
 
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.tradingview.com/*"],
      "js": ["content.js"]
    }
  ]
}
