{
  "manifest_version": 3,
  "name": "TradingView Scraper",
  "version": "1.0",
  "description": "Scrape stock data from TradingView watchlist/screener.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.tradingview.com/*"],
      "js": ["content.js"]
    }
  ]
}
